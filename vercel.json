{
    "name": "subc",
    "version": 2,
    "builds": [
        {
            "src": "subconverter/subconverter.exe",
            "use": "now-static-bin",
            "config": {
                "output": "public",
                "includeFiles": [
                    "subconverter/**"
                ]
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "public/subconverter.exe" }
    ]
}
